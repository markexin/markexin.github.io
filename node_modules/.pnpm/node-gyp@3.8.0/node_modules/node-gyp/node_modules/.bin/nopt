#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/nopt@3.0.6/node_modules/nopt/bin/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/nopt@3.0.6/node_modules/nopt/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/nopt@3.0.6/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules:/Users/maqun/Documents/fontend-note/node_modules:/Users/maqun/Documents/node_modules:/Users/maqun/node_modules:/Users/node_modules:/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node-gyp@3.8.0/node_modules/node-gyp/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node-gyp@3.8.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/nopt@3.0.6/node_modules/nopt/bin/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/nopt@3.0.6/node_modules/nopt/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/nopt@3.0.6/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules:/Users/maqun/Documents/fontend-note/node_modules:/Users/maqun/Documents/node_modules:/Users/maqun/node_modules:/Users/node_modules:/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node-gyp@3.8.0/node_modules/node-gyp/node_modules:/Users/maqun/Documents/fontend-note/app2/node_modules/.pnpm/node-gyp@3.8.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@3.0.6/node_modules/nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../../../../../nopt@3.0.6/node_modules/nopt/bin/nopt.js" "$@"
fi
